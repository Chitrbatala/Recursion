class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    int ans = pro(1234);
	    System.out.println(ans);
	}
	
	public static int pro(int n) {
	    if (n % 10 == n) return n;
	    
	    return (n % 10) * pro(n / 10);
	}
}

